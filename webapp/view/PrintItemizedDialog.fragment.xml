<core:FragmentDefinition xmlns:mvc="sap.ui.core.mvc" displayBlock="true" xmlns="sap.m" xmlns:l="sap.ui.layout" xmlns:f="sap.ui.layout.form"
	xmlns:core="sap.ui.core" controllerName="com.baba.ZDSD_CHANGESO1.controller.Worklist">
	<Dialog id="PrintItemizedDialog" title="Select UoM">
		<content>
			<l:HorizontalLayout class="sapUiContentPadding">
				<RadioButtonGroup id="rbUomType" columns="2" select="onRbAddTypeChange" selectedIndex="0">
					<buttons>
						<RadioButton id="rbItemBox" text="BOX"/>
						<RadioButton id="rbItemPc" text="PC"/>
					</buttons>
					<!--<layoutData>-->
					<!--	<l:GridData span="XL5 L5 M3 S6"/>-->
					<!--</layoutData>-->
				</RadioButtonGroup>
			</l:HorizontalLayout>
		</content>
		<beginButton>
			<Button id="btnOk" text="{i18n>BTN_OK}" press="onPrintItemizedOk"></Button>
		</beginButton>
		<endButton>
			<Button id="btnClose" text="{i18n>BTN_CLOSE}" press="onPrintItemizedClose"></Button>
		</endButton>
	</Dialog>
</core:FragmentDefinition>